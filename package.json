{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "npx prisma generate && next build",
    "postinstall": "npx prisma generate",
    "start": "next start -p 3001",
    "lint": "next lint",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:notifications:complete": "tsx scripts/migrate-notifications-complete.ts",
    "migrate:all": "npm run migrate && npm run migrate:notifications:complete",
    "seed": "tsx scripts/seed.ts",
    "db:fix": "tsx scripts/fix-db.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:create": "tsx scripts/create-db.ts",
    "db:check": "tsx scripts/test-db-connection.ts",
    "db:health": "tsx -e \"import { healthCheck } from './lib/db'; healthCheck().then(console.log).catch(console.error)\"",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "check:db": "tsx scripts/check-db.ts",
    "test:notification-system": "tsx scripts/test-notification-system.ts",
    "test:notification-api": "tsx scripts/test-notification-api.ts",
    "test:auth": "tsx scripts/test-auth-api.ts",
    "dev:clean": "npm run clean && npm run dev",
    "setup:full": "npm run dev"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@tanstack/react-query": "^5.90.20",
    "autoprefixer": "10.4.20",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "browserify-zlib": "^0.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.31.0",
    "https-browserify": "^1.0.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "0.563.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^8.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.18.0",
    "postcss": "8.4.49",
    "prisma": "^7.3.0",
    "pusher": "^5.3.2",
    "pusher-js": "^8.4.0",
    "querystring-es3": "^0.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "3.4.17",
    "tsx": "^4.21.0",
    "vm-browserify": "^1.1.2",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.11",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "@types/react": "19.2.11",
    "@types/react-dom": "19.2.3"
  }
}
