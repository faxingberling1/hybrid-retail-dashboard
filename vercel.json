{
  "version": 2,
  "buildCommand": "cd apps/dashboard && npm install && npm run build",
  "outputDirectory": "apps/dashboard/.next",
  "installCommand": "cd apps/dashboard && npm install",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/apps/dashboard/$1"
    }
  ]
}