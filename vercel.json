{
  "version": 2,
  "builds": [
    {
      "src": "apps/dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/dashboard/$1"
    }
  ],
  "buildCommand": "cd ../.. && npx turbo run build --filter=dashboard",
  "installCommand": "cd ../.. && npm install",
  "outputDirectory": "apps/dashboard/.next"
}