    "https-browserify": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "next": "14.0.3",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.13",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.17.2",
    "postcss": "^8.4.33",
    "pusher": "^5.3.2",
    "pusher-js": "^8.4.0",
    "querystring-es3": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
