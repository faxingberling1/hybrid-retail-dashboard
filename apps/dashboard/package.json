    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.3",
    "prisma": "^7.3.0",
    "typescript": "^5.3.3"
  },
